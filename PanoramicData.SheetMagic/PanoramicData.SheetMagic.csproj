<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<Authors>Panoramic Data Limited</Authors>
		<Copyright>Copyright (c) Panoramic Data Limited 2017-2018</Copyright>
		<PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
		<PackageProjectUrl>http://www.panoramicdata.com/sheetmagic/</PackageProjectUrl>
		<RepositoryUrl>https://bitbucket.org/panoramicdata/panoramicdata.sheetmagic</RepositoryUrl>
		<RepositoryType>GIT</RepositoryType>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<Description>A library for easily saving/loading generic lists to/from XLSX workbooks.
Fuzzy logic and string matching is used to work out what you meant, and it all works like magic.

If you are the kind of person that likes to be in control, you can be explicit too.</Description>
		<PackageIconUrl>http://www.panoramicdata.com/wp-content/uploads/2017/10/SheetMagic.png</PackageIconUrl>
		<Version>2.0.10</Version>
		<PackageId>PanoramicData.SheetMagic</PackageId>
		<Title>SheetMagic</Title>
		<PackageReleaseNotes>Added AddSheetOptions allowing to specify IncludeProperties or ExcludeProperties as a list of strings.</PackageReleaseNotes>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="DocumentFormat.OpenXml" Version="2.8.1" />
		<PackageReference Include="Microsoft.CSharp" Version="4.5.0" />
	</ItemGroup>
	<ItemGroup>
		<AssemblyAttribute Include="System.Runtime.CompilerServices.InternalsVisibleTo">
			<_Parameter1>$(MSBuildProjectName).Test</_Parameter1>
		</AssemblyAttribute>
	</ItemGroup>

</Project>